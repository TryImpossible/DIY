{
  "name": "LeleMarket",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "run-web": "webpack-dev-server --config webpack.config.js --progress",
    "build-web": "webpack-cli --config webpack.config.js --progress",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "lint1": "eslint App.tsx --fix",
    "precommit": "lint-staged",
    "prettier": "prettier --write src/**",
    "tsc": "node node_modules/typescript/lib/tsc.js"
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.7",
    "@react-navigation/web": "^1.0.0-alpha.9",
    "react": "16.9.0",
    "react-dom": "^16.13.0",
    "react-native": "0.61.5",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-reanimated": "^1.7.0",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "^2.4.0",
    "react-native-web": "^0.12.2",
    "react-navigation": "^4.3.2",
    "react-navigation-stack": "^2.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^0.0.7",
    "@types/jest": "^25.1.4",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-native": "^0.61.22",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-react-native-web": "^0.12.2",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "metro-react-native-babel-preset": "^0.58.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "16.9.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "url-loader": "^4.0.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "jest": {
    "preset": "react-native"
  },
  "lint-staged": {
    "ignore": [
      "package.json"
    ],
    "linters": {
      "*.{js,jsx,ts,tsx}": [
        "yarn lint",
        "git add --force"
      ],
      "*.{js,ts,tsx,json,md,graphql}": [
        "yarn prettier",
        "git add --force"
      ]
    }
  }
}
