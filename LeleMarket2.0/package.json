{
  "name": "LeleMarket",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "SET ENVFILE=.env.development && react-native run-android --appId com.lelemarket --variant devDebug",
    "ios": "SET ENVFILE=.env.development && react-native run-ios",
    "start": "SET ENVFILE=.env.development && react-native start --reset-cache --verbose",
    "dll": "webpack-cli --config ./web/config/webpack.dll.config.js --progress --inline --colors",
    "web": "webpack-dev-server --config ./web/config/webpack.config.development.js --progress --inline --colors",
    "build-web": "webpack-cli --config ./web/config/webpack.config.production.js --progress --inline --colors",
    "test": "jest",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "precommit": "lint-staged",
    "prettier": "prettier --write src/**",
    "tsc": "tsc"
  },
  "dependencies": {
    "@react-navigation/web": "^1.0.0-alpha.9",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-native": "0.62.2",
    "react-native-config": "^1.0.0",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-reanimated": "^1.8.0",
    "react-native-screens": "^2.4.0",
    "react-native-web": "^0.12.2",
    "react-navigation": "^4.3.7",
    "react-navigation-stack": "^1.10.3"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^1.0.0",
    "@types/jest": "^24.0.24",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-native": "^0.62.0",
    "@types/react-navigation": "^3.4.0",
    "@types/react-test-renderer": "16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-react-native-web": "^0.12.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.2",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.5.1",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.2.0",
    "husky": "^4.2.5",
    "jest": "^24.9.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.1.3",
    "metro-react-native-babel-preset": "^0.58.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.4",
    "react-test-renderer": "16.11.0",
    "style-loader": "^1.1.4",
    "thread-loader": "^2.1.3",
    "ts-loader": "^7.0.0",
    "typescript": "^3.8.3",
    "uglifyjs-webpack-plugin": "1",
    "url-loader": "^4.1.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "lint-staged": {
    "ignore": [
      "package.json"
    ],
    "linters": {
      "*.{js,jsx,ts,tsx}": [
        "yarn lint",
        "git add --force"
      ],
      "*.{js,ts,tsx,json,md,graphql}": [
        "yarn prettier",
        "git add --force"
      ]
    }
  },
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  }
}
